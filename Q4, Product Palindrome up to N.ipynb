   "source": [
    "#Project Euler 4\n",
    "def Palindrome(m):\n",
    "    n = 0\n",
    "    for a in range(m, 1, -1):\n",
    "        for b in range(a, 1, -1):\n",
    "            x = a * b\n",
    "            if x > n:\n",
    "                s = str(a * b)\n",
    "                if s == s[::-1]:\n",
    "                    n = a * b\n",
    "    return (n)\n",
    "\n",
    "Palindrome(999)"
   ]

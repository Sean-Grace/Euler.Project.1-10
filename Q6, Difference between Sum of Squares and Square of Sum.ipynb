   "source": [
    "#Project Euler 6\n",
    "\n",
    "def SumOfSquareDiff(n):\n",
    "    #Assigning initial values for Variables\n",
    "    x=0\n",
    "    y=0\n",
    "    #Carrying out calculations for all numbers up to n\n",
    "    for i in range(n+1):\n",
    "        x+=i\n",
    "        y+=i**2\n",
    "        \n",
    "    return(x**2-y)\n",
    "\n",
    "SumOfSquareDiff(100)"
   ]
  }
